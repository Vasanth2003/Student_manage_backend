{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/student/(.*)",
      "dest": "/api/student/$1"
    },
    { "handle": "filesystem" },
    { "src": ".*", "status": 404, "dest": "/404.html" }
  ],
  "env": {
    "MONGODB_URI": "@your-vercel-environment-variable-for-mongodb-uri"
  }
}
